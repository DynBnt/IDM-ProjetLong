    <empreinte nom="MCU_Footprint" largeur="20.0" hauteur="20.0">
      <ports nom="MCU_FP_REF"/>
    </empreinte>
    <contraintes xsi:type="catalogue:GeoGenerale" distanceMin="5.0"/>
