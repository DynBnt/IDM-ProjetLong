<?xml version="1.0" encoding="UTF-8"?>
<catalogue:Catalogue xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:catalogue="http://fr.n7.catalogue" nom="CatalogueExemple">
  <composants nom="Microcontroller">
